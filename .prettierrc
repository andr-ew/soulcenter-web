{
  "semi": true,
  "tabWidth": 4,
  "arrowParens": "avoid",
  "singleQuote": true,
  "jsxSingleQuote": true,
  "bracketSpacing": true,
  "plugins": ["prettier-plugin-go-template"],
  "overrides": [
    {
      "files": ["*.html"],
      "options": {
        "parser": "go-template"
      }
    }
  ]
}
